{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "constants/*": [
        "src/constants/*"
      ],
      "components/*": [
        "src/components/*"
      ],
      "api/*": [
        "pages/api/*"
      ],
      "request": [
        "src/request.js"
      ],
    },
    "jsx": "react",
    "module": "commonjs"
  },
  "exclude": [
    "node_modules",
    ".next"
  ]
}